 SELECT 'INCOMING' INTERCHANGE_TYPE, i_rwrd AMOUNT, decode( sign((length(card_acct_num) -15 )), 1, SUBSTR (card_acct_num, 1, 6) || RPAD ('X', LENGTH (card_acct_num) - 10, 'X') || SUBSTR (card_acct_num, -4, 4),  '', 'Unknown', '') CARD_ACCT_NUMBER FROM Party_Hierarchy ph,  Inc_Rwrd_Dtl A  WHERE A.Work_Of_Dt = '20240624' AND ph.Party_ID = A.Party_ID AND ph.Begin_WorkOfDate = A.Begin_WorkOfDate AND ph.anc_composite_id||'' LIKE 'BAMS CONVERTED%' ORDER BY card_acct_num
