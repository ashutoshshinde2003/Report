-- Run this in SQL Workshop -> SQL Commands
SELECT ws.*
FROM (
  -- pick the most recent uploaded file (no app filter so it always picks latest)
  SELECT blob_content
  FROM (
    SELECT blob_content,
           ROW_NUMBER() OVER (ORDER BY created_on DESC) rn
    FROM apex_application_temp_files
  )
  WHERE rn = 1
) f,
TABLE(
  apex_data_parser.get_xlsx_worksheets(
    p_content => f.blob_content
  )
) ws;