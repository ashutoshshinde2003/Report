-- Reset IR session state so pagination returns to page 1
apex_util.reset_report(
    p_page      => :APP_PAGE_ID,
    p_region_id => apex_util.find_region_id('VALID_RECORDS')
);

apex_util.reset_report(
    p_page      => :APP_PAGE_ID,
    p_region_id => apex_util.find_region_id('INVALID_RECORDS')
);